{
    "image": "temporalio/gitpod-edu:1.4",
    "customizations": {
        "codespaces": {
            "openFiles": [
                "README.md"
            ]
        }
    },
    "forwardPorts": [8080, 7233, 9999],
    "postStartCommand": "bash ./devcontainer/python_setup.sh",
    "postAttachCommand": "source .env/bin/activate",
    "remoteEnv": {
        "PROMPT_DIRTRIM": "2"
    }
}
